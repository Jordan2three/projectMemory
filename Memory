function flipCards (flipUp, flipDown) {
  if cardFlip1 && cardFlip2 is the same element then addPoints++ or tallyPoints;

  if number of cards flip >= 2 && cards don't match
        then all cards back to flipDown position
  
  else
  no points awarded
};


function multiPlayer (player1, player2) {
  points++;
};


function timer (if currentTimer >= 0) {
  for let i = 300; i >= 0; i--;
};
